{
  "name": "koa-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "npm run build:server && run-p watch:server dev:server",
    "build:server": "tsc -p src/server/",
    "dev:server": "node-dev --respawn dist/server/index.js",
    "watch:server": "tsc -p src/server/ -w",
    "dev:basic": "npm run build:basic && webpack --config=webpack.basic.js --watch",
    "build:basic": "webpack --config=webpack.basic.js",
    "start:dropzone": "cd src/dropzone && SKIP_PREFLIGHT_CHECK=true npm run start",
    "build:dropzone": "cd src/dropzone && SKIP_PREFLIGHT_CHECK=true npm run build",
    "install:dropzone": "cd src/dropzone && npm install",
    "textile:check": "npx ncu '/^@textile/.*$/'",
    "textile:upgrade": "npx ncu -u '/^@textile/.*$/'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@textile/hub": "^0.1.7",
    "@textile/threads-core": "^0.1.24",
    "dotenv": "^8.2.0",
    "emittery": "^0.7.0",
    "isomorphic-ws": "^4.0.1",
    "jdenticon": "^2.2.0",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-route": "^3.2.0",
    "koa-router": "^8.0.8",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^6.0.0",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.153",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "copy-webpack-plugin": "^6.0.1",
    "expose-loader": "^0.7.5",
    "lodash": "^4.17.15",
    "mocha": "^7.2.0",
    "node-dev": "^4.0.0",
    "npm-check-updates": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
